<h1>Listing carts</h1>

<table>
  <tr>
  </tr>
  
<% for cart in @carts %>
  <tr>
    <td><%= link_to 'Show', cart_path(cart) %></td>
    <td><%= link_to 'Edit', edit_cart_path(cart) %></td>
    <td><%= link_to 'Destroy', cart_path(cart), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New cart', new_cart_path %>